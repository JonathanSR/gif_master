<h1> Create an account for:</h1>
<h2>GifMaster</h2>

<%=form_for(@user) do |f| %>
  <%=f.label :name %>
  <%=f.text_field :name %>

  <%=f.label :email %>
  <%=f.text_field :email %>

  <%=f.label :password %>
  <%=f.password_field :password %>
  <%=f.label :password_confirmation %>
  <%= f.password_field :password_confirmation%>
  <%= f.submit %>

  <% end %>
  